{
  "compilerOptions": {
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "ES6",
    "jsx": "react",
    "resolveJsonModule": true,
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "sourceMap": true
  },
  "include": [
    "src"
, "src/.index.tsx"  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}
